{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "storagedatalake170925"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDataLakeStorage1170925NEW_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1170925NEW'"
		},
		"DEST170925_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DEST170925'"
		},
		"DESTCSV_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DESTCSV'"
		},
		"AzureDataLakeStorage1170925NEW_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stoadfdatalake170925.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "storagedatalake170925_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDataLakeStorage1170925NEW_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1170925NEW_accountKey')]"
					},
					"DEST170925_connectionString": {
						"value": "[parameters('DEST170925_connectionString')]"
					},
					"DESTCSV_connectionString": {
						"value": "[parameters('DESTCSV_connectionString')]"
					},
					"AzureDataLakeStorage1170925NEW_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1170925NEW_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}